#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int limit,i,values[1000];

	printf("Enter a limit  ");
	scanf("%d",&limit);

	printf("Enter values  ");

	for (i=0;i<limit;i++)
	{scanf("%d",&values[i]);}






	return EXIT_SUCCESS;
}
