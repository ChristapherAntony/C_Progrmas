#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i, j;

	for(i=1; i<=12; i++){
		for(j=1; j<=6; j++){
			if(j==1|| i==1 && j==2|| i==1 && j==3  || i==5&&j==1 ||i==5&&j==2 ||i==5&&j==3 ||i==5&&j==4||i==5&&j==5 ||i==5&&j==6 ||
					i==6&&j==1 ||i==6&&j==2 ||i==6&&j==3 ||i==6&&j==4||i==6&&j==5 ||i==6&&j==6 ||
					i==12&&j==2 ||i==12&&j==3 ||i==12&&j==4 ||i==12&&j==5 ||i==12&&j==6){
				printf("*  ");
			}
		}printf("\n\n");
	}

	return EXIT_SUCCESS;
}
