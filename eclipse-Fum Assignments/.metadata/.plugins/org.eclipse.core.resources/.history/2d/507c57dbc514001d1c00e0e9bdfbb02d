#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int sizeOfArray;
	int i, j;
	int array1[3][3], array2[3][3];

	printf("Enter the size of arrays");
	scanf("%i",sizeOfArray);

	printf("\nEnter the values of array 1");
	for(i=0; i<sizeOfArray; i++){
		for(j=0; j<sizeOfArray; j++){
			scanf("%d",& array1[i][j]);
		}
	}
	for(i=0; i<sizeOfArray; i++){
		for(j=0; j<sizeOfArray; j++){
			printf("%d", array1[i][j]);
		}
	}





	return EXIT_SUCCESS;
}
