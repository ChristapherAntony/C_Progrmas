#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int i, j,n=10, a[100]={12,45,23,48,75,10,45,99,55,77};
	int temp=0;
	//reverse an array
	for(i=0; i<=n/2; i++){
		temp=a[i];
		a[i]=a[n-1];
		a[n-1]=temp;

	}
	printf("reversed array\n");

	for(i=0; i<n; i++){
		printf( "%d  ", a[i]);
	}




	return EXIT_SUCCESS;
}
