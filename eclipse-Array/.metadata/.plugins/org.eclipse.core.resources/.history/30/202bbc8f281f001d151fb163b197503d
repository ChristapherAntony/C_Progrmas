#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int i, j, n=5, a[100]={1,4,6,8,3};
	int flag;
	// array deeltion
	// prime number deletion
	for(i=0; i<n; i++){
		flag=0;
		for(j=1; i<n; j++){
			if(a[i]%j==0){
				flag=1;
			}
		}

		}




	return EXIT_SUCCESS;
}
