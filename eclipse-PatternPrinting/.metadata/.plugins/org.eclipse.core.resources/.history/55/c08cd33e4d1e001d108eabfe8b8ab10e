#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);

	int n=5, i, j, length=(n*2)-1, start=1, end=length;

	for(i=1; i<=length; i++){
		for(j=1; j<=length; j++){
			printf("* ");
		}
		printf("\n");
	}



	return EXIT_SUCCESS;
}
