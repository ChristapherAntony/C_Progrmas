#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int n=20, num=1, i;


	for(i=1; i<=n; i++){
		if(i%4==0 || i%2==1){
			printf("%d ",i);
		}

	}
	return EXIT_SUCCESS;
}
